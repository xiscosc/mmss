{
  "name": "mss",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "prepare": "husky install",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check --log-level warn .",
    "format:fix": "prettier --write --log-level warn .",
    "openapi": "openapi-typescript open-api.v1.json --output ./src/generated/mss.openapi.ts"
  },
  "devDependencies": {
    "@swc/core": "1.3.102",
    "@swc/helpers": "0.5.3",
    "@tsconfig/node18": "18.2.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/aws-lambda": "8.10.119",
    "@types/jest": "29.5.4",
    "@types/jest-when": "3.5.2",
    "@types/lambda-log": "3.0.1",
    "@types/node": "20.5.6",
    "@types/uuid": "9.0.7",
    "aws-cdk": "2.118.0",
    "esbuild": "0.19.11",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unicorn": "48.0.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "jest-when": "3.6.0",
    "lint-staged": "15.2.0",
    "openapi-typescript": "6.5.3",
    "prettier": "3.1.1",
    "regenerator-runtime": "0.14.1",
    "ts-jest": "29.1.1",
    "ts-node": "https://github.com/TypeStrong/ts-node",
    "typescript": "5.3.3"
  },
  "overrides": {
    "tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.489.0",
    "@aws-sdk/lib-dynamodb": "3.489.0",
    "aws-cdk-lib": "2.118.0",
    "bignumber.js": "9.1.2",
    "constructs": "10.2.70",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.1.0",
    "lambda-log": "3.1.0",
    "source-map-support": "0.5.21",
    "uuid": "9.0.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,yml,yaml,json}": [
      "npx prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "npx eslint --fix"
    ]
  }
}
